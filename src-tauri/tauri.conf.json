{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "pnpm build",
    "beforeDevCommand": "pnpm dev",
    "devPath": "http://localhost:5173",
    "distDir": "../dist"
  },
  "package": {
    "productName": "overseers-manual-df",
    "version": "../package.json"
  },
  "plugins": {
    "fs": {
      "scope": ["**"]
    },
    "updater": {
      "endpoints": ["https://tauri-update-server.nwest.workers.dev/v1/{{target}}/{{arch}}/{{current_version}}"]
    }
  },
  "tauri": {
    "bundle": {
      "active": true,
      "category": "Reference",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"],
      "identifier": "games.nwest.dwarffortress.overseer-manual",
      "longDescription": "A utility to make it easier to get details about the creatures and other things represented in raws for your Dwarf Fortress saves.",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "Overseer's Reference Manual for Dwarf Fortress",
      "targets": "all",
      "updater": {
        "active": true,
        "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDU1REM2MTc2RjQxRDI0QgpSV1JMMGtGdkY4WmRCYnh5TXFCZmlqY0FONHFWRXhVZlNhSDhwL3RUb3Y2MzkrZFBYYThvWHppcgo="
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "wix": {
          "template": "custom.wxs"
        }
      }
    },
    "security": {
      "assetProtocol": {
        "enable": true,
        "scope": ["**"]
      },
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 640,
        "resizable": true,
        "title": "Overseer's Reference Manual for Dwarf Fortress",
        "width": 944
      }
    ]
  }
}
