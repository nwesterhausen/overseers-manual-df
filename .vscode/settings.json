{
  "conventionalCommits.promptScopes": false,
  "rust-analyzer.checkOnSave.overrideCommand": [
    "cargo",
    "clippy",
    "--message-format=json",
    "--",
    "-D",
    "clippy::pedantic",
    "-D",
    "clippy::nursery",
    "-D",
    "clippy::unwrap_used",
    "-D",
    "clippy::expect_used",
    "-D",
    "unsafe_code",
    "-W",
    "missing_docs",
    "-W",
    "unreachable_code",
    "-W",
    "unreachable_patterns",
    "-A",
    "clippy::used_underscore_binding",
  ],
  "cSpell.words": [
    "bestiarum",
    "dbaeumer",
    "dfraw",
    "dotenv",
    "dotenvy",
    "esbenp",
    "Ferdinandi",
    "fract",
    "gamelog",
    "kobalte",
    "lyrasearch",
    "mikestead",
    "scrolltop",
    "serayuzgur",
    "shrubland",
    "solidjs",
    "tamasfe",
    "vivaxy",
    "vocabulum"
  ],
  "githubPullRequests.ignoredPullRequestBranches": [
    "main"
  ],
  "css.lint.unknownAtRules": "ignore",
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "dist": true,
    "**/target": true,
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "dist": true,
    "**/node_modules": true,
  }
}
