{
  "conventionalCommits.promptScopes": false,
  "rust-analyzer.checkOnSave.overrideCommand": [
    "cargo",
    "clippy",
    "--message-format=json",
    "--",
    "-W",
    "clippy::pedantic",
    "-W",
    "clippy::nursery",
    "-W",
    "clippy::unwrap_used",
    "-W",
    "clippy::expect_used",
    "-A",
    "clippy::too_many_lines"
  ],
  "cSpell.words": [
    "bestiarum",
    "dbaeumer",
    "dfraw",
    "dotenv",
    "dotenvy",
    "esbenp",
    "Ferdinandi",
    "gamelog",
    "kobalte",
    "lyrasearch",
    "scrolltop",
    "shrubland",
    "solidjs",
    "vivaxy",
    "vocabulum"
  ],
  "githubPullRequests.ignoredPullRequestBranches": ["main"],
  "css.lint.unknownAtRules": "ignore"
}
